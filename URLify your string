#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
 {
	//code
	int test;cin>>test;
	cin.get();
	while(test){
	    string str1,str2("%20");
	    getline(cin,str1);int len;cin>>len;
	    
	    for(int i=0;i<=len;i++){
	        if(str1[i]==' '){
	            str1.erase(i,1);
	            str1.insert(i,str2);
	            len+=3;
	        }
	    }
	    cout<<str1<<endl;
	    cin.get();
	    test--;
	}
	return 0;
}
